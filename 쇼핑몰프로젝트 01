#include <stdio.h>

int main() {
    int a, b, c;

   
    printf("상품 개수를 입력하세요: ");
    scanf_s("%d", &a);  

    printf("상품 ID를 입력하세요: ");
    scanf_s("%d", &b); 
    printf("판매 수량을 입력하세요: ");
    scanf_s("%d", &c);  

    
    printf("입력된 상품 개수: %d\n", a);
    printf("입력된 상품 ID: %d\n", b);
    printf("입력된 판매 수량: %d\n", c);

    return 0;
}
